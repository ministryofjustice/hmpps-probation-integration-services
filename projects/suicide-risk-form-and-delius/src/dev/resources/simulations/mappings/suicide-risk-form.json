{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPattern": "/suicide-risk-form/pdf/.+"
      },
      "response": {
        "status": 200,
        "bodyFileName": "test.pdf",
        "headers": {
          "Content-Type": "application/octet-stream"
        }
      }
    },
    {
      "request": {
        "method": "GET",
        "url": "/suicide-risk-form/pdf/404"
      },
      "response": {
        "status": 404
      }
    },
    {
      "request": {
        "method": "GET",
        "url": "/suicide-risk-form/pdf/invalid"
      },
      "response": {
        "status": 200,
        "body": "INVALID",
        "headers": {
          "Content-Type": "application/octet-stream"
        }
      }
    }
  ]
}