{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPathPattern": "/flipt-enabled/api/v1/flags/(.*)"
      },
      "response": {
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200,
        "body": "{\"enabled\": true}"
      }
    },
    {
      "request": {
        "method": "GET",
        "urlPathPattern": "/flipt-disabled/api/v1/flags/(.*)"
      },
      "response": {
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200,
        "body": "{\"enabled\": false}"
      }
    }
  ]
}
