{
  "role": {
    "name": "$SERVICE_NAME"
  },
  "customDimensions": {
    "service.version": "${VERSION}",
    "service.team": "probation-integration"
  },
  "instrumentation": {
    "logging": {
      "level": "DEBUG"
    },
    "springScheduling": {
      "enabled": false
    }
  },
  "selfDiagnostics": {
    "destination": "console"
  }
}